{
  "name": "babel-plugin-proptypes-require",
  "version": "1.0.0",
  "description": "Babel plugin that adds PropTypes import declaration if file contains JSX tags.",
  "keywords": [
    "babel",
    "babel-plugin",
    "react",
    "jsx"
  ],
  "license": "MIT",
  "author": {
    "name": "Scott Tolinski",
    "email": "scott.tolinski@gmail.com",
    "url": "https://twitter.com/stolinski"
  },
  "main": "lib",
  "scripts": {
    "clean": "rimraf coverage lib",
    "build": "babel src -d lib",
    "build-examples": "babel examples -d built-examples",
