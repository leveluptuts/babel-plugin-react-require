    "@babel/register": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.15.0",
